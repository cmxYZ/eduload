 = keycloak.tokenParsed.user.person.title;\n    },\n    mouseMove() {\n      if (typeof keycloak.tokenParsed != 'undefined' && keycloak.tokenParsed.user.person.title != this.name) {\n        console.log(\"update\");\n        this.name = keycloak.tokenParsed.user.person.title;\n        this.isAuth = true;\n      } else {\n        console.log(\"Something is Happening\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["Header","Grid","keycloak","components","data","gridColumns","gridData","ФИО","Должность","Ставка","Год","methods","login","update","console","log","tokenParsed","name","user","person","title","mouseMove","isAuth"],"sources":["D:\\Diploma\\domains\\eduload.vue\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"app\">\r\n    <Header></Header>\r\n    <span class=\"reg\">{{ name }}</span>\r\n    <button class=\"reg\" @click=\"login\">Sign In</button>\r\n    <Grid :data=\"gridData\" :columns=\"gridColumns\"></Grid>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/Header.vue\";\r\nimport Grid from \"@/components/Grid.vue\";\r\nimport keycloak from \"./main\";\r\n\r\nexport default {\r\n  components: {\r\n    Header,\r\n    Grid,\r\n  },\r\n  data() {\r\n    return {\r\n      gridColumns: [\r\n        \"ФИО\",\r\n        \"Место работы\",\r\n        \"Должность\",\r\n        \"Ставка\",\r\n        \"Часы на ставку\",\r\n        \"Планируемое количеаство часов\",\r\n        \"Фактическое количество часов\",\r\n        \"Разница между плановым и фактическим количеством часов\",\r\n        \"Год\",\r\n      ],\r\n      gridData: [\r\n        {\r\n          ФИО: \"Сапожникова Александра Вячеславовна\",\r\n          \"Место работы\":\r\n            \"Кафедра иностранных языков и образовательных технологий\",\r\n          Должность: \"Старший преподаватель\",\r\n          Ставка: \"1.0\",\r\n          \"Часы на ставку\": \"100\",\r\n          \"Планируемое количеаство часов\": \"64\",\r\n          \"Фактическое колич