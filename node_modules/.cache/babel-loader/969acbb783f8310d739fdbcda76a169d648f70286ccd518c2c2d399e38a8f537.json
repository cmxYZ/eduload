Hour\", width: 120, cellRenderer: params => {\r\n            if (params.value == 1) {\r\n              return '<div><input type=\"checkbox\" checked></div>';\r\n            }\r\n            else {\r\n              return '<div><input type=\"checkbox\"></div>';\r\n            }\r\n          },},\r\n      ],\r\n      defaultColDef: {\r\n        sortable: true,\r\n        resizable: true,\r\n        spanHeaderHeight: true,\r\n        wrapHeaderText: true,\r\n        minWidth: 50,\r\n        wrapText: true,\r\n        autoHeight: true,\r\n        autoHeaderHeight: true,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      window.history.back();\r\n    },\r\n    async fetchJson() {\r\n      let json;\r\n      await fetch(\"http://212.193.88.196:8080/loaddatabytkey?tkey=\" + this.$route.query.tkey)\r\n          .then((resp) => resp.text())\r\n          .then((razm) => (json = razm));\r\n      this.gridData = JSON.parse(json);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.table {\r\n  margin: 20px;\r\n  display: flex;\r\n  height: 850px;\r\n  width: 2180px;\r\n}\r\n</style>"],"mappings":"AAiBA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,iBAAgB,MAAO,2BAA2B;AAEzD,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,EAAE;IAChB,IAAI,CAACC,QAAO,GAAIH,iBAAiB;IACjC,IAAI,CAACI,IAAG,GAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,IAAI;IAClC,IAAI,CAACG,IAAG,GAAI,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,IAAI;IAClC,IAAI,CAACC,IAAG,GAAI,IAAI,CAACH,MAAM,CAACC,KAAK,CAACE,IAAI;IAClC,IAAI,CAACC,KAAI,GAAI,IAAI,CAACJ,MAAM,CAACC,KAAK,CAACG,KAAK;EACtC,CAAC;EACDC,UAAU,EAAE;IACVX;EACF,CAAC;EACDY,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,CACV;QAAEC,UAAU,EAAE,GAAG;QAAEC,WAAW,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAI,CAAC,EACvD;QAAEF,UAAU,EAAE,YAAY;QAAEG,KAAK,EAAE,gBAAgB;QAAED,KAAK,EAAE,GAAG;QAAEE,MAAM,EAAE,IAAI;QAAEC,cAAc,EAAE;MAAK,CAAC,EACrG;QAAEL,UAAU,EAAE,sBAAsB;QAAEG,KAAK,EAAE,eAAe;QAAED,KAAK,EAAE,GAAG;QAAEE,MAAM,EAAE,IAAI;QAAEC,cAAc,EAAE;MAAK,CAAC,EAC9G;QAAEL,UAAU,EAAE,SAAS