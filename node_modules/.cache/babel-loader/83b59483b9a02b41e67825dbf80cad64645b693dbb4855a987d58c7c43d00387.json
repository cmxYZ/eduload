{"ast":null,"code":"import Header from \"@/components/Header.vue\";\nimport Keycloak from \"keycloak-js\";\nimport Welcome from \"@/components/Welcome.vue\";\nconst keycloak = new Keycloak({\n  url: \"https://keys.urfu.ru/auth/\",\n  realm: \"urfu-lk\",\n  clientId: \"rtf-eduload\",\n  public_key: \"\",\n  \"token-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/protocol/openid-connect\",\n  \"account-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/account\",\n  \"tokens-not-before\": 0\n});\nexport default {\n  mounted() {\n    this.updateKC();\n  },\n  components: {\n    Header,\n    Welcome\n  },\n  data() {\n    return {\n      role: 'norole',\n      showSingIn: true,\n      name: \"Not Auth\",\n      isAuth: false\n    };\n  },\n  methods: {\n    async updateKC() {\n      await keycloak.init({\n        onLoad: \"check-sso\"\n      });\n      if (keycloak.authenticated) {\n        this.name = keycloak.tokenParsed.user.person.title;\n        this.showSingIn = false;\n        let url = 'http://212.193.88.196:8080/checkuser?login=' + keycloak.tokenParsed.user.person.email + '&isKeycloak=1&samAccountName=' + keycloak.tokenParsed.user.person.id;\n        console.log(url);\n        await fetch(url).then(resp => resp.text()).then(razm => this.role = razm);\n      }\n    },\n    async login() {\n      if (!keycloak.authenticated) {\n        await keycloak.login();\n      } else {\n        this.name = keycloak.tokenParsed.user.person.title;\n        this.isAuth = true;\n      }\n    },\n    async logout() {\n      if (keycloak.authenticated) {\n        await keycloak.logout();\n      }\n    }\n  }\n};","map":{"version":3,"names":["Header","Keycloak","Welcome","keycloak","url","realm","clientId","public_key","mounted","updateKC","components","data","role","showSingIn","name","isAuth","methods","init","onLoad","authenticated","tokenParsed","user","person","title","email","id","console","log","fetch","then","resp","text","razm","login","logout"],"sources":["D:\\Diploma\\domains\\eduload.vue\\src\\App.vue"],"sourcesContent":["<template>\r\n  <Header></Header>\r\n  <div class=\"w-page\" v-if=\"showSingIn\">\r\n    <Welcome></Welcome>\r\n    <button class=\"signin-button\" @click=\"login\">Войти с помощью учетной записи УрФУ</button>\r\n  </div>\r\n  <span class=\"reg\" v-if=\"!showSingIn\">{{ name }}</span>\r\n  <button class=\"reg-button\" @click=\"logout\" v-if=\"!showSingIn\">Выйти</button>\r\n  <router-view :  v-if=\"!showSingIn\"></router-view>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/Header.vue\";\r\nimport Keycloak from \"keycloak-js\";\r\nimport Welcome from \"@/components/Welcome.vue\";\r\n\r\nconst keycloak = new Keycloak({\r\n  url: \"https://keys.urfu.ru/auth/\",\r\n  realm: \"urfu-lk\",\r\n  clientId: \"rtf-eduload\",\r\n  public_key:\r\n      \"\",\r\n  \"token-service\":\r\n      \"https://keys.urfu.ru/auth/realms/urfu-lk/protocol/openid-connect\",\r\n  \"account-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/account\",\r\n  \"tokens-not-before\": 0,\r\n});\r\n\r\nexport default {\r\n  mounted() {\r\n    this.updateKC();\r\n  },\r\n  components: {\r\n    Header,\r\n    Welcome\r\n  },\r\n  data() {\r\n    return {\r\n      role: 'norole',\r\n      showSingIn: true,\r\n      name: \"Not Auth\",\r\n      isAuth: false,\r\n    };\r\n  },\r\n  methods: {\r\n    async updateKC()\r\n    {\r\n      await keycloak.init({\r\n        onLoad: \"check-sso\",\r\n      });\r\n      if (keycloak.authenticated) {\r\n        this.name = keycloak.tokenParsed.user.person.title;\r\n        this.showSingIn = false;\r\n        let url = 'http://212.193.88.196:8080/checkuser?login=' + keycloak.tokenParsed.user.person.email + '&isKeycloak=1&samAccountName=' + keycloak.tokenParsed.user.person.id;\r\n        console.log(url);\r\n        await fetch(url).then((resp) => resp.text())\r\n            .then((razm) => (this.role = razm));\r\n      }\r\n    },\r\n    async login() {\r\n      if (!keycloak.authenticated) {\r\n        await keycloak.login();\r\n      } else {\r\n        this.name = keycloak.tokenParsed.user.person.title;\r\n        this.isAuth = true;\r\n      }\r\n    },\r\n    async logout() {\r\n      if (keycloak.authenticated) {\r\n        await keycloak.logout();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n.signin-button {\r\n  padding: 10px;\r\n  border-radius: 10px;\r\n  margin-left: 18%;\r\n  margin-top: 15px;\r\n  cursor: pointer;\r\n  display: inline-flex;\r\n  width: 200px;\r\n  font-size: 16px;\r\n}\r\n.w-page{\r\n  display: flex;\r\n  align-items: center;\r\n\r\n}\r\n.reg-button {\r\n  padding: 10px;\r\n  border-radius: 10px;\r\n  font-size: 18px;\r\n  margin-left: 20px;\r\n  margin-top: 15px;\r\n  cursor: pointer;\r\n  display: inline-flex;\r\n}\r\n\r\n.reg {\r\n  display: inline-flex;\r\n  margin-top: 15px;\r\n  margin-left: 20px;\r\n  font-size: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AAYA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,aAAa;AAClC,OAAOC,OAAM,MAAO,0BAA0B;AAE9C,MAAMC,QAAO,GAAI,IAAIF,QAAQ,CAAC;EAC5BG,GAAG,EAAE,4BAA4B;EACjCC,KAAK,EAAE,SAAS;EAChBC,QAAQ,EAAE,aAAa;EACvBC,UAAU,EACN,0YAA0Y;EAC9Y,eAAe,EACX,kEAAkE;EACtE,iBAAiB,EAAE,kDAAkD;EACrE,mBAAmB,EAAE;AACvB,CAAC,CAAC;AAEF,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,EAAE;EACjB,CAAC;EACDC,UAAU,EAAE;IACVV,MAAM;IACNE;EACF,CAAC;EACDS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,QAAQ;MACdC,UAAU,EAAE,IAAI;MAChBC,IAAI,EAAE,UAAU;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMP,QAAQA,CAAA,EACd;MACE,MAAMN,QAAQ,CAACc,IAAI,CAAC;QAClBC,MAAM,EAAE;MACV,CAAC,CAAC;MACF,IAAIf,QAAQ,CAACgB,aAAa,EAAE;QAC1B,IAAI,CAACL,IAAG,GAAIX,QAAQ,CAACiB,WAAW,CAACC,IAAI,CAACC,MAAM,CAACC,KAAK;QAClD,IAAI,CAACV,UAAS,GAAI,KAAK;QACvB,IAAIT,GAAE,GAAI,6CAA4C,GAAID,QAAQ,CAACiB,WAAW,CAACC,IAAI,CAACC,MAAM,CAACE,KAAI,GAAI,+BAA8B,GAAIrB,QAAQ,CAACiB,WAAW,CAACC,IAAI,CAACC,MAAM,CAACG,EAAE;QACxKC,OAAO,CAACC,GAAG,CAACvB,GAAG