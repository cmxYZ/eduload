{"ast":null,"code":"import Header from \"@/components/Header.vue\";\nimport Keycloak from \"keycloak-js\";\nconst keycloak = new Keycloak({\n  url: \"https://keys.urfu.ru/auth/\",\n  realm: \"urfu-lk\",\n  clientId: \"rtf-eduload\",\n  public_key: \"\",\n  \"token-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/protocol/openid-connect\",\n  \"account-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/account\",\n  \"tokens-not-before\": 0\n});\nexport default {\n  mounted() {\n    this.updateKC();\n  },\n  components: {\n    Header\n  },\n  data() {\n    return {\n      showSingIn: true,\n      name: \"Not Auth\",\n      isAuth: false,\n      searchQuery: '',\n      gridData: []\n    };\n  },\n  methods: {\n    async updateKC() {\n      await keycloak.init({\n        onLoad: \"check-sso\"\n      });\n      if (keycloak.authenticated) {\n        this.name = keycloak.tokenParsed.user.person.title;\n        this.showSingIn = false;\n        let url = 'http://212.193.88.196:8080/adduser?login=' + keycloak.tokenParsed.user.person.email + '&isKeycloak=1&samAccountName=' + keycloak.tokenParsed.user.person.id;\n        console.log(url);\n        fetch(url).then(data => {\n          console.log(data);\n        });\n      }\n    },\n    async login() {\n      if (!keycloak.authenticated) {\n        await keycloak.login();\n      } else {\n        this.name = keycloak.tokenParsed.user.person.title;\n        this.isAuth = true;\n      }\n    },\n    async logout() {\n      if (keycloak.authenticated) {\n        await keycloak.logout();\n      }\n    }\n  }\n};","map":{"version":3,"names":["Header","Keycloak","keycloak","url","realm","clientId","public_key","mounted","updateKC","components","data","showSingIn","name","isAuth","searchQuery","gridData","methods","init","onLoad","authenticated","tokenParsed","user","person","title","email","id","console","log","fetch","then","login","logout"],"sources":["D:\\Diploma\\domains\\eduload.vue\\src\\App.vue"],"sourcesContent":["<template>\r\n  <Header></Header>\r\n  <span class=\"reg mt-110\">{{ name }}</span>\r\n  <button class=\"reg\" @click=\"login\" v-if=\"showSingIn\">Войти</button>\r\n  <button class=\"reg\" @click=\"logout\" v-if=\"!showSingIn\">Выйти</button>\r\n  <router-view v-if=\"!showSingIn\"></router-view>\r\n  <!--v-if=\"!showSingIn\"-->\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/Header.vue\";\r\nimport Keycloak from \"keycloak-js\";\r\n\r\nconst keycloak = new Keycloak({\r\n  url: \"https://keys.urfu.ru/auth/\",\r\n  realm: \"urfu-lk\",\r\n  clientId: \"rtf-eduload\",\r\n  public_key:\r\n      \"\",\r\n  \"token-service\":\r\n      \"https://keys.urfu.ru/auth/realms/urfu-lk/protocol/openid-connect\",\r\n  \"account-service\": \"https://keys.urfu.ru/auth/realms/urfu-lk/account\",\r\n  \"tokens-not-before\": 0,\r\n});\r\n\r\nexport default {\r\n  mounted() {\r\n    this.updateKC();\r\n  },\r\n  components: {\r\n    Header,\r\n  },\r\n  data() {\r\n    return {\r\n      showSingIn: true,\r\n      name: \"Not Auth\",\r\n      isAuth: false,\r\n      searchQuery: '',\r\n      gridData: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async updateKC()\r\n    {\r\n      await keycloak.init({\r\n        onLoad: \"check-sso\",\r\n      });\r\n      if (keycloak.authenticated) {\r\n        this.name = keycloak.tokenParsed.user.person.title;\r\n        this.showSingIn = false;\r\n        let url = 'http://212.193.88.196:8080/adduser?login=' + keycloak.tokenParsed.user.person.email + '&isKeycloak=1&samAccountName=' + keycloak.tokenParsed.user.person.id;\r\n        console.log(url);\r\n        fetch(url).then((data) => {\r\n          console.log(data);\r\n        })}\r\n    },\r\n    async login() {\r\n      if (!keycloak.authenticated) {\r\n        await keycloak.login();\r\n      } else {\r\n        this.name = keycloak.tokenParsed.user.person.title;\r\n        this.isAuth = true;\r\n      }\r\n    },\r\n    async logout() {\r\n      if (keycloak.authenticated) {\r\n        await keycloak.logout();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.reg {\r\n  display: flex;\r\n  margin-top: 15px;\r\n  margin-left: 15px;\r\n  font-size: 20px;\r\n}\r\n.table {\r\n  margin: 20px;\r\n  display: flex;\r\n  height: 900px;\r\n  width: 2360px;\r\n}\r\n\r\nheader img {\r\n  height: 100px;\r\n}\r\n\r\nheader {\r\n  width: 100%;\r\n  height: 100px;\r\n  background-color: rgb(221, 221, 221);\r\n  flex: 1 100px;\r\n  display: flex;\r\n  flex-flow: row;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0px 50px;\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n}\r\n\r\nheader button {\r\n  display: inline-block;\r\n  height: 50px;\r\n  width: 50px;\r\n}\r\n\r\nheader .logotext {\r\n  margin-left: 10px;\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n}\r\n\r\nheader .logo {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\nheader .menu {\r\n  height: 49px;\r\n  width: 49px;\r\n}\r\n\r\n.mt-110 {\r\n  margin-top: 110px;\r\n}\r\n</style>\r\n"],"mappings":"AAUA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,aAAa;AAElC,MAAMC,QAAO,GAAI,IAAID,QAAQ,CAAC;EAC5BE,GAAG,EAAE,4BAA4B;EACjCC,KAAK,EAAE,SAAS;EAChBC,QAAQ,EAAE,aAAa;EACvBC,UAAU,EACN,0YAA0Y;EAC9Y,eAAe,EACX,kEAAkE;EACtE,iBAAiB,EAAE,kDAAkD;EACrE,mBAAmB,EAAE;AACvB,CAAC,CAAC;AAEF,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,EAAE;EACjB,CAAC;EACDC,UAAU,EAAE;IACVT;EACF,CAAC;EACDU,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI;MAChBC,IAAI,EAAE,UAAU;MAChBC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMR,QAAQA,CAAA,EACd;MACE,MAAMN,QAAQ,CAACe,IAAI,CAAC;QAClBC,MAAM,EAAE;MACV,CAAC,CAAC;MACF,IAAIhB,QAAQ,CAACiB,aAAa,EAAE;QAC1B,IAAI,CAACP,IAAG,GAAIV,QAAQ,CAACkB,WAAW,CAACC,IAAI,CAACC,MAAM,CAACC,KAAK;QAClD,IAAI,CAACZ,UAAS,GAAI,KAAK;QACvB,IAAIR,GAAE,GAAI,2CAA0C,GAAID,QAAQ,CAACkB,WAAW,CAACC,IAAI,CAACC,MAAM,CAACE,KAAI,GAAI,+BAA8B,